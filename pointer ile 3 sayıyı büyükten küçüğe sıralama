#include <stdio.h>
int yerdegistir(int*,int*);
int main(){
    int a,b,c;
    printf("Ã¼c sayi girin:  ");
    scanf("%d%d%d",&a,&b,&c);
    if(a<b)
    yerdegistir(&a,&b);
    if(a<c)
    yerdegistir(&a,&c);
    if(b<c)
    yerdegistir(&b,&c);
    printf("sayilarin sira: %d%d%d",a,b,c);
return 0;
}
int yerdegistir(int *x,int*z){
    int gecici;
    gecici=*x;
    *x=*z;
    *z=gecici;
}
